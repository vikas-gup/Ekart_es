<h1>Listing items</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Brand</th>
    <th>String</th>
    <th>Price</th>
    <th>Float</th>
    <th>Category</th>
    <th>Integer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @items.each do |item| %>
  <tr>
    <td><%= item.name %></td>
    <td><%= item.brand %></td>
    <td><%= item.string %></td>
    <td><%= item.price %></td>
    <td><%= item.float %></td>
    <td><%= item.category_id %></td>
    <td><%= item.integer %></td>
    <td><%= link_to 'Show', item %></td>
    <td><%= link_to 'Edit', edit_item_path(item) %></td>
    <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Item', new_item_path %>
